      <xs:element ref="ucore:AttachmentLinkReference" />
      <xs:element minOccurs="0" name="Descriptor" type="ucore:StringType">
        <xs:annotation>
          <xs:documentation>A free text description of the association between a thing and an attachmentLink</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="AttachmentLinkReference" type="ulexlib:ValidatingReferenceType">
    <xs:annotation>
      <xs:documentation>A validating reference to an AttachmentLink in this DataItemPackage.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="BinaryAttachment" substitutionGroup="ulex:AttachmentContentAbstract" type="ucore:BinaryAttachmentType">
    <xs:annotation>
      <xs:documentation>A binary data attachment with the binary content in base64.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ThingAttachmentLinkAssociation" type="ucore:ThingAttachmentLinkAssociationType">
    <xs:annotation>
      <xs:documentation>Creates a link between a Thing in the Digest, and an AttachmentLink within this DataItemPackage.</xs:documentation>
    </xs:annotation>
  </xs:element>
