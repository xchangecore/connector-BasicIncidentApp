<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:ICISM="urn:us:gov:ic:ism:v2" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:ulex-codes="ulex:message:codes:1.0" xmlns:ddms="http://metadata.dod.mil/mdr/ns/DDMS/2.0/" xmlns:ucore="http://ucore.gov/ucore/2.0" xmlns:ulexlib="ulex:message:library:1.0" xmlns:ulex="ulex:message:structure:1.0" xmlns:ulexpd="ulex:message:pd:1.0" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://ucore.gov/ucore/2.0" version="2.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="../../import/ULEX1.0PD/xsd/ulex/ulex/1.0/ulex.xsd" namespace="ulex:message:structure:1.0" />
  <xs:import schemaLocation="../../import/ULEX1.0PD/xsd/ulex/ulex-codes/1.0/ulex-codes.xsd" namespace="ulex:message:codes:1.0" />
  <xs:import schemaLocation="../../import/ULEX1.0PD/xsd/ulex/ulex-library/1.0/ulex-library.xsd" namespace="ulex:message:library:1.0" />
  <xs:import schemaLocation="../../import/ICISM_2.1/IC-ISM-v2.1.xsd" namespace="urn:us:gov:ic:ism:v2" />
  <xs:import schemaLocation="../../import/DDMS_2.0/ns/DDMS/2.0/DDMS-v2_0.xsd" namespace="http://metadata.dod.mil/mdr/ns/DDMS/2.0/" />
  <xs:import schemaLocation="../../import/UCore_GML_Profile_2.0/ucore_gml_profile.xsd" namespace="http://www.opengis.net/gml/3.2" />
  <xs:complexType name="ContentMetadataType">
    <xs:annotation>
      <xs:documentation>The basic set of metadata that is available to all elements.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="Comment" type="ucore:StringType">
        <xs:annotation>
          <xs:documentation>Amplifying comments go here.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="InformationSource" type="ddms:publisherType">
        <xs:annotation>
          <xs:documentation>The source for the information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ValidityTime" type="ucore:TimeType">
        <xs:annotation>
          <xs:documentation>The time instant or interval for which the data is considered valid. If not set, the data is assumed to be valid indefinitely.   If it is set, then the data should be considered invalid or stale for times other than that specified by ValidityTime.  </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
  </xs:complexType>
  <xs:attribute name="code" type="xs:token">
    <xs:annotation>
      <xs:documentation>The machine readable description of a concept located within the codespace identified using the codespace attribute.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="codespace" type="xs:anyURI">
    <xs:annotation>
      <xs:documentation>A qualifier that specifies the namespace of the source vocabulary for this identifier (e.g., http://abc.com/ or urn:com:xyz).  Note that if the URI is provided as a URL, it does not imply that the attachment is available over the Internet at that address. However, the UCore team strongly recommends that the codespace value be a resolvable URL, if possible. Each class term in a codespace is identified by its URI.  The URI of the class is yielded as follows: if the codespace attribute ends in '#' or '/', then the URI of the class CLASS-URI = @codespace @code.  If the codespace attribute does not end in '#' or '/', then the URI of the class CLASS-URI = @codespace  # @code.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="decimalErrorBound" type="xs:decimal">
    <xs:annotation>
      <xs:documentation>This describes the uncertainty associated with a numeric value.  This is a single value that defines a symmetric interval of 2 times the value.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="integerErrorBound" type="xs:int">
    <xs:annotation>
      <xs:documentation>This descibes the uncertainty associated with a numeric value in integer units. This is a single value that defines a symmetric interval of 2 times the value.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="label">
    <xs:annotation>
      <xs:documentation>The human readable representation of the concept that corresponds to the codespace and code attributes, if they are provided.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:complexType name="BooleanType">
    <xs:annotation>
      <xs:documentation>A boolean value with limited associated metadata.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:boolean">
        <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="DecimalType">
    <xs:annotation>
      <xs:documentation>A decimal value that has limited associated metadata.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:double">
        <xs:annotation>
          <xs:documentation>A decimal value that has limited associated metadata</xs:documentation>
        </xs:annotation>
        <xs:attribute ref="ucore:decimalErrorBound">
          <xs:annotation>
            <xs:documentation>This describes the error bound associated with the value.  The error bound is a symmetric interval of 2 times the attribute value.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="ExtendedStringType">
    <xs:annotation>
      <xs:documentation>An extension to the basic UCore StringType that allows for any number of alternate spellings to be added.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Value" type="ucore:StringType">
        <xs:annotation>
          <xs:documentation>The actual string value.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="AltSpelling" type="ucore:StringType">
        <xs:annotation>
          <xs:documentation>Possible alternate spellings for the same string.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="IdentifierType">
    <xs:annotation>
      <xs:documentation>A means of identification for some Thing.  For example, an identifier for a vehicle might be a Vehicle Identification Number (VIN) with a value of  1M8GDM9AXKP042788.  For an employee, an organization may assign an Employee Identification Number with a value of 30621.   In the cyber world, screennames provide a mechanism for identification.  These would all be appropriate uses of the ucore:IdentifierType.  The ucore:IdentifierType provides a means to reuse terms and definitions from other codespaces via the codespace/code attribute pair and also provides a human-readable format via the label attribute.  The ucore:IdentifierType should only be used to represent simple identifiers that answer the quiestion "Which Entity or Event is this?"  For simple properties in general, the SimplePropertyType should be used instead to preserve the semantic richness of the ucore:IdentifierType.  Each class term in a codespace is identified by its URI.  The URI of the class is yielded as follows: if the codespace attribute ends in '#' or '/', then the URI of the class CLASS-URI = @codespace @code.  If the codespace attribute does not end in '#' or '/', then the URI of the class CLASS-URI = @codespace  # @code.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
        <xs:attribute ref="ucore:code" />
        <xs:attribute ref="ucore:codespace" />
        <xs:attribute ref="ucore:label" use="required" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="IntegerType">
    <xs:annotation>
      <xs:documentation>An integer value that has limited associated metadata.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:int">
        <xs:annotation>
          <xs:documentation>An integer value that has limited associated metadata</xs:documentation>
        </xs:annotation>
        <xs:attribute ref="ucore:integerErrorBound">
          <xs:annotation>
            <xs:documentation>This descibes the uncertainty associated with a numeric value in integer units. The error bound is a symmetric interval of 2 times the attribute value.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="RoleType">
    <xs:annotation>
      <xs:documentation>The role of a thing in a relationship, with code/codespace pair that allows a data producer to define roles from external codespaces. Each class term in a codespace is identified by its URI.  The URI of the class is yielded as follows: if the codespace attribute ends in '#' or '/', then the URI of the class CLASS-URI = @codespace @code.  If the codespace attribute does not end in '#' or '/', then the URI of the class CLASS-URI = @codespace  # @code.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
        <xs:attribute ref="ucore:code" />
        <xs:attribute ref="ucore:codespace" />
        <xs:attribute ref="ucore:label" use="required" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="SimplePropertyType">
    <xs:annotation>
      <xs:documentation>An essential or distinctive attribute or quality of a Thing. The ucore:SimplePropertyType provides a means to reuse terms and definitions from other codespaces via the codespace/code attribute pair and also provides a human-readable format via the label attribute.  The ucore:SimplePropertyType should only be used to represent properties that are self-contained to a Thing (e.g., color of a car, the number of rooms in a building,  the type of propulsion system on a vessel), NOT for properties that imply relationships to other things.   For properties that imply relationships to other things, a relationship with associated roles should be used instead. Each class term in a codespace is identified by its URI.  The URI of the class is yielded as follows: if the codespace attribute ends in '#' or '/', then the URI of the class CLASS-URI = @codespace @code.  If the codespace attribute does not end in '#' or '/', then the URI of the class CLASS-URI = @codespace  # @code.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
        <xs:attribute ref="ucore:code" />
        <xs:attribute ref="ucore:codespace" />
        <xs:attribute ref="ucore:label" use="required" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="StringType">
    <xs:annotation>
      <xs:documentation>A string of characters with limited associated metadata.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="TimeInstantType">
    <xs:annotation>
      <xs:documentation>A time instant that includes uncertainty.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:TimeValueType">
        <xs:sequence>
          <xs:element minOccurs="0" name="UncertaintyInterval" type="xs:duration">
            <xs:annotation>
              <xs:documentation>The uncertainty in the time instant.  This uncertainty interval is +/- the value in this duration element.  If this element contains P30M, the uncertainty on the time is +/- 30 minutes.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TimeIntervalType">
    <xs:annotation>
      <xs:documentation>Represents an interval between two specified instants in time.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="StartTime" type="ucore:TimeInstantType">
        <xs:annotation>
          <xs:documentation>The start instant of the interval, with associated uncertainty.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="EndTime" type="ucore:TimeInstantType">
        <xs:annotation>
          <xs:documentation>The end instant of the interval, with associated uncertainty.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TimeType">
    <xs:annotation>
      <xs:documentation>The basic time representation type, allowing for either an interval or an instant in time to be represented, including uncertainty.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="ucore:TimeAbstract" />
    </xs:sequence>
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
  </xs:complexType>
  <xs:complexType name="TimeValueType">
    <xs:annotation>
      <xs:documentation>A fixed point in time, with no uncertainty.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Value">
        <xs:annotation>
          <xs:documentation>A time.  This element is a union of xs:date, xs:time, xs:dateTime, xs:gDay, xs:gMonth, xs:gMonthDay, xs:gYear, and xs:gYearMonth. This element must match the format corresponding to one of those simple types.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union memberTypes="xs:date xs:time xs:dateTime xs:gDay xs:gMonth xs:gMonthDay xs:gYear xs:gYearMonth" />
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element abstract="true" name="TimeAbstract">
    <xs:annotation>
      <xs:documentation>The abstract head of a substitution group for the representation of time.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="TimeInterval" substitutionGroup="ucore:TimeAbstract" type="ucore:TimeIntervalType">
    <xs:annotation>
      <xs:documentation>An interval in time, defined by two instants in time.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="TimeInstant" substitutionGroup="ucore:TimeAbstract" type="ucore:TimeInstantType">
    <xs:annotation>
      <xs:documentation>An instant in time that includes uncertainty.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="AngleUOMType">
    <xs:annotation>
      <xs:documentation>Angle UOM</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="radian" />
      <xs:enumeration value="degree [unit of angle]" />
      <xs:enumeration value="minute [unit of angle]" />
      <xs:enumeration value="second [unit of angle]" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LengthUOMType">
    <xs:annotation>
      <xs:documentation>Length/Distance UOM</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="metre" />
      <xs:enumeration value="kilometre" />
      <xs:enumeration value="nautical mile" />
      <xs:enumeration value="foot" />
      <xs:enumeration value="mile (statute mile)" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="MassUOMType">
    <xs:annotation>
      <xs:documentation>Mass/Weight UOM</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="kilogram" />
      <xs:enumeration value="gram" />
      <xs:enumeration value="pound" />
      <xs:enumeration value="ounce" />
      <xs:enumeration value="ton (US)" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="VelocityUOMType">
    <xs:annotation>
      <xs:documentation>Velocity UOM</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="metre per second" />
      <xs:enumeration value="kilometre per hour" />
      <xs:enumeration value="knot" />
      <xs:enumeration value="foot per second" />
      <xs:enumeration value="mile per hour" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AngleMeasureType">
    <xs:annotation>
      <xs:documentation>An angle measurement, with an associated error bound.  The units on the error bound are assumed to be the same as those on the measurement.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:DecimalType">
        <xs:attribute name="angleUOM" type="ucore:AngleUOMType" use="required">
          <xs:annotation>
            <xs:documentation>A unit of measurement assigned to this angle.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LengthMeasureType">
    <xs:annotation>
      <xs:documentation>A length measurement, with an associated error bound.  The units on the error bound are assumed to be the same as those on the measurement.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:DecimalType">
        <xs:attribute name="lengthUOM" type="ucore:LengthUOMType" use="required">
          <xs:annotation>
            <xs:documentation>A unit of measurement assigned to this length.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="MassMeasureType">
    <xs:annotation>
      <xs:documentation>A mass measurement, with an associated error bound.  The units on the error bound are assumed to be the same as those on the measurement.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:DecimalType">
        <xs:attribute name="massUOM" type="ucore:MassUOMType" use="required">
          <xs:annotation>
            <xs:documentation>A unit of measurement assigned to this mass.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="VelocityMeasureType">
    <xs:annotation>
      <xs:documentation>A velocity measurement, with an associated error bound.  The units on the error bound are assumed to be the same as those on the measurement.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:DecimalType">
        <xs:attribute name="velocityUOM" type="ucore:VelocityUOMType" use="required">
          <xs:annotation>
            <xs:documentation>A unit of measurement assigned to this velocity.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AgentType">
    <xs:annotation>
      <xs:documentation> Base type for any agent (WHO).</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:EntityType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CircleByCenterPointType">
    <xs:annotation>
      <xs:documentation>A wrapper type for GML CircleByCenterPoint.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="gml:CircleByCenterPoint" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CollectionType">
    <xs:annotation>
      <xs:documentation> A named collection of other Things, including one or more What terms.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:ThingType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" ref="ucore:What">
            <xs:annotation>
              <xs:documentation>Codespace/value pair that defines what this Collection is. Each Collection must include at least one term from the UCore Taxonomy, with the associated UCore codespace of http://ucore.gov/2.0/codespace/.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="ThingRef" type="ucore:ThingRefType">
            <xs:annotation>
              <xs:documentation>References to the Things that make up this collection.  An empty collection is valid, and is treated as a named set with 0 elements.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ContactInfoType">
    <xs:annotation>
      <xs:documentation>Contact information for a person or organization.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Email" type="xs:string">
        <xs:annotation>
          <xs:documentation>Email address for this contact.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Phone" type="xs:string">
        <xs:annotation>
          <xs:documentation>Phone number for this contact.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CyberAddressType">
    <xs:annotation>
      <xs:documentation>Any address within the cyber domain, from DDMS.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="ddms:virtualCoverage" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EntityType">
    <xs:annotation>
      <xs:documentation>Base type for an Entity.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:ThingType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" ref="ucore:What">
            <xs:annotation>
              <xs:documentation>Codespace/value pair that defines what this Entity is. Each Entity must include at least one term from the UCore Taxonomy, with the associated UCore codespace, http://ucore.gov/2.0/codespace/.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EnvelopeType">
    <xs:annotation>
      <xs:documentation>A wrapper type for GML Envelope.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="gml:Envelope" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EventType">
    <xs:annotation>
      <xs:documentation>Base type for an Event.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:ThingType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" ref="ucore:What">
            <xs:annotation>
              <xs:documentation>Codespace/value pair that defines what this Event is. Each Event must include at least one term from the UCore Taxonomy, with the associated UCore codespace, http://ucore.gov/2.0/codespace/.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GeoLocationType">
    <xs:annotation>
      <xs:documentation>A physical location with coordinates, or a simple geospatial region.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="ucore:GeoLocationAbstract" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LineStringType">
    <xs:annotation>
      <xs:documentation>A wrapper type for GML LineString.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="gml:LineString" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LocationType">
    <xs:annotation>
      <xs:documentation>The base UCore location type, aggregates all other location types.  Allows either 0-N CyberAddresses, or 0-N PhysicalAddresses AND GeoLocations.  If multiple locations are included in a single element, they must refer to the same logical location. (For example, the address and coordinates of the White House.)</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:ThingType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ucore:CyberAddress">
            <xs:annotation>
              <xs:documentation>Any address within the cyber domain, from DDMS. Must NOT be used in the same element as GeoLocation and PhysicalAddress.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ucore:GeoLocation" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="ucore:PhysicalAddress" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OrganizationType">
    <xs:annotation>
      <xs:documentation>Represents an organization.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:AgentType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="AlternateName" type="ucore:ExtendedStringType">
            <xs:annotation>
              <xs:documentation>Any alternate names or acronyms for this organization.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ContactInfo" type="ucore:ContactInfoType">
            <xs:annotation>
              <xs:documentation>A point of contact for this organization.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="Name" type="ucore:ExtendedStringType">
            <xs:annotation>
              <xs:documentation>The name of the organization.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PersonNameType">
    <xs:annotation>
      <xs:documentation>The name of a person.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
