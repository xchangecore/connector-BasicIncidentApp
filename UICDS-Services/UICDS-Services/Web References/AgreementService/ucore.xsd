      <xs:extension base="ucore:RelationshipType">
        <xs:sequence>
          <xs:element minOccurs="2" maxOccurs="2" name="ThingRef" type="ucore:ThingRefType">
            <xs:annotation>
              <xs:documentation>The things to which this relationship applies.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="AffiliatedWith" substitutionGroup="ucore:RelationshipAbstract" type="ucore:ThingThingRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford (derived): attached or connected with. closely associated with.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CauseOf" substitutionGroup="ucore:RelationshipAbstract" type="ucore:CauseOfRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford: be the cause of; make happen.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Controls" substitutionGroup="ucore:RelationshipAbstract" type="ucore:AgentEntityRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford: have control or command of.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DistinctFrom" substitutionGroup="ucore:RelationshipAbstract" type="ucore:ThingThingRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford: recognizably different or individual.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="EmployedBy" substitutionGroup="ucore:RelationshipAbstract" type="ucore:PersonAgentRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford: give work to (someone) and pay them for it.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="HasDestinationOf" substitutionGroup="ucore:RelationshipAbstract" type="ucore:EntityLocationRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford (derived): the location to which an entity is going or being sent.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="HasFamilialRelationTo" substitutionGroup="ucore:RelationshipAbstract" type="ucore:PersonPersonRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford (derived): a person who is related to another by birth or by marriage.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="HasOriginOf" substitutionGroup="ucore:RelationshipAbstract" type="ucore:EntityLocationRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford (derived): the location where an entity begins or arises.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="InvolvedIn" substitutionGroup="ucore:RelationshipAbstract" type="ucore:AgentEventRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford (derived): to experience or participate in an activity or situation.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="LocatedAt" substitutionGroup="ucore:RelationshipAbstract" type="ucore:EntityLocationExtendedRelationshipType">
    <xs:annotation>
      <xs:documentation>The extended Entity-Location relationship that includes kinematics.  This indicates an entity is at a specific location. </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="OccursAt" substitutionGroup="ucore:RelationshipAbstract" type="ucore:EventLocationRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford (derived): happen; take place at. OR exist or be found to be present at a location.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element abstract="true" name="RelationshipAbstract" type="ucore:RelationshipType">
    <xs:annotation>
      <xs:documentation>The abstract head of the substitution group for all relationships.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="SameAs" substitutionGroup="ucore:RelationshipAbstract" type="ucore:ThingThingRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford: the same thing as previously mentioned. (the same) identical people or things. exactly alike; not different or changed.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="SubordinateTo" substitutionGroup="ucore:RelationshipAbstract" type="ucore:SubordinateSuperiorRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford: lower in rank or position.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="WorksAt" substitutionGroup="ucore:RelationshipAbstract" type="ucore:PersonLocationRelationshipType">
    <xs:annotation>
      <xs:documentation>Oxford: the location at which a person performs a task or tasks to be undertaken.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AttachmentResourceMetadataType">
    <xs:annotation>
      <xs:documentation>This is an extended type that allows an attachment to be marked with IC-ISM security markings, and as a DDMS Resource.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ddms:ResourceType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AttachmentSecurityMetadataType">
    <xs:annotation>
      <xs:documentation>This type allows for IC-ISM markings on ULEX attachments.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
  </xs:complexType>
  <xs:complexType name="DataItemStatusType">
    <xs:annotation>
      <xs:documentation>A type that defines the status of this message.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
    <xs:attribute ref="ucore:code" />
    <xs:attribute ref="ucore:codespace" />
    <xs:attribute ref="ucore:label" use="required" />
  </xs:complexType>
  <xs:complexType name="DigestMetadataType">
    <xs:annotation>
      <xs:documentation>Defines the metadata for the UCore Digest</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
  </xs:complexType>
  <xs:complexType name="DigestType">
    <xs:annotation>
      <xs:documentation>The DigestType definition.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" ref="ucore:DigestMetadata" />
      <xs:sequence maxOccurs="unbounded">
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ucore:ThingAbstract" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ucore:RelationshipAbstract" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ucore:ThingAttachmentLinkAssociation" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ulexlib:SameAsPayloadAssociation" />
      </xs:sequence>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MessageClassificationType">
    <xs:annotation>
      <xs:documentation>Defines the classification of this UCore message using the IC-ISM security markings.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
  </xs:complexType>
  <xs:complexType name="PackageResourceMetadataType">
    <xs:annotation>
      <xs:documentation>Defines the UCore extension to the ULEX Package metadata to allow the package to be treated as a DDMS resource</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ddms:ResourceType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PackageSecurityMetadataType">
    <xs:annotation>
      <xs:documentation>Defines the UCore extension to the ULEX Package metadata to allow for IC-ISM security markings.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
  </xs:complexType>
  <xs:element name="AttachmentResourceMetadataExtension" substitutionGroup="ulex:AttachmentExtensionAbstract" type="ucore:AttachmentResourceMetadataType">
    <xs:annotation>
      <xs:documentation>This element treats the attachment as a DDMS Resource, and is in the ulex:AttachmentExtensionAbstract subGrp.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AttachmentSecurityMetadata" substitutionGroup="ulex:AttachmentExtensionAbstract" type="ucore:AttachmentSecurityMetadataType">
    <xs:annotation>
      <xs:documentation>The global AttachementSecurityMetadata element for integration with the ULEX framework.  This element is in the ulex:AttachmentExtensionAbstract subGrp.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DataItemStatus" substitutionGroup="ulex:DataItemStatusAbstract" type="ucore:DataItemStatusType">
    <xs:annotation>
      <xs:documentation>A type that defines the status of this message.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DataOwnerContact" substitutionGroup="ulex:DataOwnerContactAbstract" type="ddms:pointOfContactType">
    <xs:annotation>
      <xs:documentation>Placeholder for progam-specific DataOwnerContact elements, such as contact name or phone number.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DataOwnerIdentifier" substitutionGroup="ulex:DataOwnerIdentifierAbstract" type="ddms:pointOfContactType">
    <xs:annotation>
      <xs:documentation>Placeholder for progam-specific DataOwnerIdentifier elements that identify an organization and a system in an organization that owns data. This identifier is used in DataOwnerMetadata that is part of PackageMetadata, and in DataOwnerInformation that is part of DataOwnersResponseMessage.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DigestMetadata" type="ucore:DigestMetadataType">
    <xs:annotation>
      <xs:documentation>Defines the metadata for the UCore Digest</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Digest" substitutionGroup="ulex:DigestAbstract" type="ucore:DigestType">
    <xs:annotation>
      <xs:documentation>The global digest element for integration with the ULEX message framework.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="DisseminationCriteria" substitutionGroup="ulex:DisseminationCriteriaAbstract">
    <xs:annotation>
      <xs:documentation>Placeholder for program-specific information provided by the data owner to indicate who is allowed to see the information.  For federated search it provides information to the service provider about what can be returned as the result of a search. For search or get results, it provides information to the user so they know the criteria for this information.  Note that this is different from the data sensitivity included at the message level.  While required for ULEX, this is superseded by the IC-ISM markings at various levels of the message.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
    </xs:complexType>
  </xs:element>
  <xs:element name="MessageClassification" substitutionGroup="ulex:DataSensitivityAbstract" type="ucore:MessageClassificationType">
    <xs:annotation>
      <xs:documentation>Defines the classification of this UCore message using the IC-ISM security markings.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Narrative" substitutionGroup="ulex:NarrativeAbstract" type="ucore:StringType">
    <xs:annotation>
      <xs:documentation>The global narrative element for integration with the ULEX message framework.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PackageResourceMetadataExtension" substitutionGroup="ulex:PackageMetadataExtensionAbstract" type="ucore:PackageResourceMetadataType">
    <xs:annotation>
      <xs:documentation>This element treats the Package as a DDMS Resource, and is in the ulex:PackageMetadataExtensionAbstract subGrp.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PackageSecurityMetadata" substitutionGroup="ulex:PackageMetadataExtensionAbstract" type="ucore:PackageSecurityMetadataType">
    <xs:annotation>
      <xs:documentation>The global PackageSecurityMetadata element for integration with the ULEX framework.  This element is in the ulex:PackageMetadataExtensionAbstract subGrp.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="SystemContact" substitutionGroup="ulex:SystemContactAbstract" type="ddms:pointOfContactType">
    <xs:annotation>
      <xs:documentation>Contact information for the system owner. Includes a person and organization to contact and their phone number and email address.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="SystemIdentifier" substitutionGroup="ulex:SystemIdentifierAbstract" type="ucore:StringType">
    <xs:annotation>
      <xs:documentation>Element that uniquely identifies an organization or system from which this data is being sent.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AttachmentContentType">
    <xs:annotation>
      <xs:documentation>The generic attachment content type, which all UCore-specific attachment types should extend.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="id" type="xs:ID" />
    <xs:attributeGroup ref="ICISM:SecurityAttributesOptionGroup" />
  </xs:complexType>
  <xs:complexType name="BinaryAttachmentType">
    <xs:annotation>
      <xs:documentation>A data type for a digital representation of an object encoded in a binary format.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="ucore:AttachmentContentType">
        <xs:sequence>
          <xs:element name="BinaryContent" type="xs:base64Binary">
            <xs:annotation>
              <xs:documentation>The actual content, base 64 binary.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="BinaryDescription" type="xs:string">
            <xs:annotation>
              <xs:documentation>A description of this binary object.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="BinarySize" type="xs:int">
            <xs:annotation>
              <xs:documentation>Size of the binary object in kilobytes (kB).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="Category" type="xs:string">
            <xs:annotation>
              <xs:documentation>The type of encoded object, e.g. XSLT, XML, PNG, JPG.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
